{
  "extends": ["standard", "plugin:prettier/recommended"],
  "env": {
    "browser": false,
    "node": true
  },
  "plugins": ["prettier"],
  "rules": {
    "no-dupe-args": "error",
    "no-unused-vars": "error",
    "prettier/prettier": "error",
    "no-console": "warn",
    "quotes": ["error", "backtick"]
  }
}
